import SynPSG.System

main
    record
        b64String       ,string
        oldString       ,a256
    endrecord

proc
    open(1,O,'TT:')
    oldString = "This is a test."
    writes(1,"Original String:")
    writes(1,%atrim(oldString))
    writes(1,"")
    b64String = Convert.ToBase64String(%atrim(oldString))
    writes(1,"Converted String:")
    writes(1,b64String)
    close 1
.ifdef DBLNET
    StopMessage()
.endc ;DBLNET
end
